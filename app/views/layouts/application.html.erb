                Вы вошли как <%= current_user.login %>
                <%= link_to 'Выйти', session_path('current_user'), method: :delete %>
              <% else %>
                <form class="navbar-form pull-left">
                  <a class="btn btn-info" href="#">Войти</a>
                </form>
              <% end %>
            </div>
          </div>
        </div>
      </div>
