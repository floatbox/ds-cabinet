}
.h-7-section-5 .additional-links {
  font-size: 12px;
  color: #bdc3c7;
  margin: 10px 0 38px;
}
.h-7-section-5 .additional-links a {
  color: #7f8c8d;
  text-decoration: underline;
}
.h-7-section-5 .additional-links a:hover,
